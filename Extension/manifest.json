{
  "manifest_version": 2,
  "name": "Image Privacy Protection Helper",
  "version": "1.0",
  "description": "A Chrome plugin implementation of the Fawkes Image Privacy Protection Framework",
  "permissions": ["activeTab", "declarativeContent", "storage", "http://127.0.0.1/*"],
  "browser_action": {
	"default_icon": {
      "16": "images/fawkes_16.png",
      "32": "images/fawkes_32.png",
      "48": "images/fawkes_48.png",
      "128": "images/fawkes_128.jpg"
    },	
	"default_popup": "popup.html"	
  }
}
